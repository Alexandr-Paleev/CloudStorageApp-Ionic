{
  "name": "cloud-storage-app",
  "version": "2.1.0",
  "author": "Aleksandr Paleev",
  "homepage": "https://github.com/aleksandrpaleev/CloudStorageApp-Ionic",
  "repository": {
    "type": "git",
    "url": "https://github.com/aleksandrpaleev/CloudStorageApp-Ionic.git"
  },
  "license": "MIT",
  "keywords": [
    "cloud-storage",
    "file-upload",
    "file-management",
    "ionic",
    "react",
    "pwa",
    "progressive-web-app",
    "typescript",
    "supabase",
    "cloudinary",
    "capacitor",
    "mobile-app",
    "open-source",
    "tanstack-query",
    "vercel"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,scss}\"",
    "deploy": "npm run build && firebase deploy --only hosting",
    "generate:icons": "node scripts/generate-icons.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/lib-storage": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/keyboard": "^6.0.0",
    "@capacitor/share": "^6.0.4",
    "@capacitor/status-bar": "^6.0.0",
    "@ionic/react": "^8.0.0",
    "@sentry/react": "^10.32.1",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.89.0",
    "@tanstack/react-query": "^5.56.0",
    "@vercel/node": "^5.5.16",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "cloudinary": "^1.41.3",
    "esbuild": "0.27.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^6.26.0",
    "vitest": "^3.2.4",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0",
    "@ionic/prettier-config": "^4.0.0",
    "@types/node": "^20.19.32",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@types/trusted-types": "^2.0.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "prettier": "^3.3.3",
    "sharp": "^0.34.5",
    "typescript": "5.5.4",
    "vite": "^5.4.0",
    "vite-plugin-pwa": "^1.2.0",
    "workbox-window": "^7.4.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "description": "Scalable SaaS Cloud Storage App with Ionic + Supabase + Cloudflare R2"
}
